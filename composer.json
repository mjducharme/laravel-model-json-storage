{
    "name": "mjducharme/laravel-model-json-storage",
    "description": "Storing your models in a json file rather than in database (single or few lines recording) can be a good option. This package saves you to create a table for a ridiculous amount of lines, improves the data recovery performances, and allows you to store and access to your models from a json files as you would do it in database.",
    "keywords": [
        "okipa",
        "package",
        "laravel",
        "php",
        "model",
        "json",
        "storage"
    ],
    "homepage": "https://github.com/mjducharme/laravel-model-json-storage",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur LORENT",
            "email": "arthur.lorent@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Okipa\\LaravelModelJsonStorage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okipa\\LaravelModelJsonStorage\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Okipa\\LaravelModelJsonStorage\\ModelJsonStorageServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
